{
  "name": "filebrowser",
  "version": "1.0.0",
  "description": "File Browser",
  "main": "index.js",
  "author": "Altan Unsal <altanunsal@gmail.com>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "handlers",
    "server",
    "client"
  ],
  "scripts": {
    "start": "concurrently --prefix \"[{name}]\" --names \"Server,Client\" --prefix-colors \"green,cyan\" \"yarn --cwd server start\" \"yarn --cwd client start\""
  },
  "dependencies": {
    "concurrently": "^6.2.1"
  }
}
